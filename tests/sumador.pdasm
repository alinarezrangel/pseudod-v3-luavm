PDVM 1.0
PLATFORM "pdcrt"

SECTION "code"
  LOCAL 0
  LOCAL 1
  OPNFRM EACT, NIL, 0
  CLSFRM EACT

  MKCLZ EACT, 1
  LSET 0

  ICONST 5
  LGET 0
  MSG 0, 1, 1
  ICONST 4
  ROT 1
  MSG 0, 1, 1
  MSG 0, 0, 1
  PRN
  NL

  ICONST 8
  LGET 0
  MSG 0, 1, 1
  LSET 1

  ICONST 2
  LGET 1
  MSG 0, 1, 1
  MSG 0, 0, 1
  PRN
  NL
  ICONST -4
  LGET 1
  MSG 0, 1, 1
  MSG 0, 0, 1
  PRN
  NL
ENDSECTION

SECTION "procedures"
  PROC 1
    PARAM 0
    OPNFRM EACT, ESUP, 1
    EINIT EACT, 0, 0
    CLSFRM EACT
    MKCLZ EACT, 2
    RETN 1
  ENDPROC

  PROC 2
    PARAM 0
    OPNFRM EACT, ESUP, 1
    EINIT EACT, 0, 0
    CLSFRM EACT
    MKCLZ EACT, 3
    RETN 1
  ENDPROC

  PROC 3
    OPNFRM EACT, ESUP, 0
    CLSFRM EACT
    LGETC EACT, 2, 0
    LGETC EACT, 1, 0
    SUM
    RETN 1
  ENDPROC
ENDSECTION

SECTION "constant pool"
  #0 STRING "llamar"
ENDSECTION
