PDVM 1.0
PLATFORM "pdcrt"

SECTION "code"
  LOCAL 0
  OPNFRM EACT, NIL, 1
  EINIT EACT, 0, 0
  CLSFRM EACT
  MKCLZ EACT, 1
  LSETC EACT, 0, 0
  LGETC EACT, 0, 0
  ICONST 20
  ROT 1
  DYNCALL 1, 1
  ROT 0
  MSG 0, 0, 1
  PRN
  NL
ENDSECTION

SECTION "procedures"
  PROC 1
    PARAM 0
    OPNFRM EACT, ESUP, 0
    CLSFRM EACT
    LGET 0
    ICONST 2
    LT
    CHOOSE 1, 2
    NAME 1
    ICONST 1
    RETN 1
    NAME 2
    LGETC EACT, 1, 0
    LGET 0
    ICONST 1
    SUB
    ROT 1
    DYNCALL 1, 1
    LGETC EACT, 1, 0
    LGET 0
    ICONST 2
    SUB
    ROT 1
    DYNCALL 1, 1
    SUM
    RETN 1
  ENDPROC
ENDSECTION

SECTION "constant pool"
  #0 STRING "comoTexto"
ENDSECTION
