PDVM 1.0
PLATFORM "pdcrt"

SECTION "code"
  LOCAL 0
  LOCAL 1
  OPNFRM EACT, NIL, 1
  EINIT EACT, 0, 0
  CLSFRM EACT
  ICONST 3
  LSETC EACT, 0, 0
  MKCLZ EACT, 1
  LSET 1
  ICONST 30
  LGET 1
  MSG 0, 1, 1
  PRN
  NL
ENDSECTION

SECTION "procedures"
  PROC 1
    PARAM 0
    LOCAL 1
    OPNFRM EACT, ESUP, 1
    EINIT EACT, 0, 1
    CLSFRM EACT
    ICONST 7
    LSETC EACT, 0, 0
    LGETC EACT, 0, 0
    LGETC EACT, 1, 0
    SUM
    ICONST 5
    SUM
    LSETC EACT, 0, 0
    LGETC EACT, 0, 0
    ICONST 5
    SUB
    RETN 1
  ENDPROC
ENDSECTION

SECTION "constant pool"
  #0 STRING "llamar"
ENDSECTION
